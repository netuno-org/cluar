{
    "name": "cluar",
    "description": "A ready to use solution for content management and multilingual websites using Netuno, ReactJS and Ant Design.",
    "type": "app",
    "config": {
        "development": {
            "cron": {"jobs": []},
            "settings": {
                "public": {},
                "cluar": {
                    "website": {
                        "url": "http://localhost:3000",
                        "name": "Website Name",
                        "analytics": null,
                        "mapbox": {
                            "dark": false,
                            "accessToken": null
                        },
                        "services": {
                            "api": "http://localhost:9000/services/"
                        }
                    },
                    "uglifyjs": false
                }
            },
            "cors": [{
                "origins": [ "*" ],
                "enabled": true
            }],
            "smtp": {},
            "name": "cluar",
            "setup": {
                "schema": {
                    "execution": true,
                    "auto_create": true
                },
                "scripts": {"execution": true},
                "enabled": true
            },
            "language": "en_GB",
            "firebase": {
                "app_name": "",
                "database_url": "",
                "key_file": "",
                "listener_secret": ""
            },
            "locale": "en_GB",
            "remote": {},
            "commands": [
                {
                    "path": "ui",
                    "command": "npm run watch",
                    "install": "npm install --force",
                    "enabled": true
                },
                {
                    "path": "website",
                    "command": "npm start",
                    "install": "npm install --force",
                    "enabled": true
                }
            ],
            "db": {"default": {
                "password": "",
                "engine": "h2",
                "port": "",
                "host": "",
                "name": "cluar",
                "username": ""
            }}
        },
        "production": {
